{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ES6", "DOM"],
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
//    "outDir": "../ts-outdir",
    "typeRoots": ["../node_modules/@types"]
  },
  "exclude": [
      "../node_modules",
      "../e2e",
      "../dist",
      "../src/assets",
      "../backup"
  ]
}
